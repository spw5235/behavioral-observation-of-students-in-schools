
  <div class="row">
    <div class="col-xs-12">
      <h2>Interval Results</h2>
      <table id="interval-results-summary" class="table obs-summary-table table-hover">
        <thead>
          <tr>
            <th class="phone-hidden">Interval</th>
            <th>AET</th>
            <th>PET</th>
            <th>OFTM</th>
            <th>OFTV</th>
            <th>OFTP</th>
            <th class="hidden-xs">Comment</th>
          </tr>
        </thead>
        <tbody>
        {{#each observations as |observation|}}
            <tr>
              <td class="display-obs-num phone-hidden">{{observation.obs_num}}</td>
              <td class="display-obs-aet">
                {{#if observation.aet}}
                  &#10004;
                {{/if}}
              </td>
              <td class="display-obs-pet">
                {{#if observation.pet}}
                  &#10004;
                {{/if}}
              </td>
              <td class="display-obs-oft-m">
                {{#if observation.oft_m}}
                  &#10004;
                {{/if}}
              </td>
              <td class="display-obs-oft-v">
                {{#if observation.oft_v}}
                  &#10004;
                {{/if}}
              </td>
              <td class="display-obs-oft-p">
                {{#if observation.oft_p}}
                  &#10004;
                {{/if}}
              </td>
              <td class="hidden-xs display-obs-obs-comment">
                {{observation.obs_comment}}
              </td>
            </tr>
        {{/each}}
        </tbody>
      </table>
    </div>
  </div>
