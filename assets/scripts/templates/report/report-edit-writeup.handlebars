<form id="report-edit-writeup-form" name="report-writeup-form">
  <fieldset>
    <h1 class="form-h1">Edit Writeup Form</h1>

    <p class="field-description">Presenting Issues</p>
    <textarea class="field-input comments-input" name="report[presenting_issue]" placeholder="Presenting Issue">{{report.presenting_issue}}</textarea>
    <br>

    <p class="field-description">Class Behavioral Assessment</p>
    <textarea class="field-input comments-input" name="report[class_behav_assess]" placeholder="Class Behavioral Assessment">{{report.class_behav_assess}}</textarea>
    <br>

    <p class="field-description">Session Setting</p>
    <textarea class="field-input comments-input" name="report[r_setting]" placeholder="Session Setting">{{report.r_setting}}</textarea>
    <br>

    <p class="field-description">Active Engaged Time</p>
    <textarea class="field-input comments-input" name="report[r_aet]" placeholder="Active Engaged Time">{{report.r_aet}}</textarea>
    <br>

    <p class="field-description">Passive Engaged Time</p>
    <textarea class="field-input comments-input" name="report[r_pet]" placeholder="Passive Engaged Time">{{report.r_pet}}</textarea>
    <br>

    <p class="field-description">Off-Task Motor Behavior</p>
    <textarea class="field-input comments-input" name="report[r_oftm]" placeholder="Off-Task Motor Behavior">{{report.r_oftm}}</textarea>
    <br>

    <p class="field-description">Off-Task Verbal Behavior</p>
    <textarea class="field-input comments-input" name="report[r_oftv]" placeholder="Off-Task Verbal Behavior">{{report.r_oftv}}</textarea>
    <br>

    <p class="field-description">Off-Task Passive Behavior</p>
    <textarea class="field-input comments-input" name="report[r_oftp]" placeholder="Off-Task Passive Behavior">{{report.r_oftp}}</textarea>
    <br>

    <p class="field-description">Findings</p>
    <textarea class="field-input comments-input" name="report[r_finding]" placeholder="Findings">{{report.r_finding}}</textarea>
    <br>

    <p class="field-description">Custom Field Title One</p>
    <textarea class="field-input comments-input" name="report[custom_one_title]" placeholder="Custom Title One">{{report.custom_one_title}}</textarea>
    <br>

    <p class="field-description">Custom Field One</p>
    <textarea class="field-input comments-input" name="report[r_customone]" placeholder="Custom Field One">{{report.r_customone}}</textarea>
    <br>

    <p class="field-description">Custom Field Title Two</p>
    <textarea class="field-input comments-input" name="report[custom_two_title]" placeholder="Custom Title Two">{{report.custom_two_title}}</textarea>
    <br>

    <p class="field-description">Custom Field Two</p>
    <textarea class="field-input comments-input" name="report[r_customtwo]" placeholder="Custom Field Two">{{report.r_customtwo}}</textarea>
    <br>

    <p class="field-description">Custom Field Title Three</p>
    <textarea class="field-input comments-input" name="report[custom_three_title]" placeholder="Custom Title Three">{{report.custom_three_title}}</textarea>
    <br>

    <p class="field-description">Custom Field Three</p>
    <textarea class="field-input comments-input" name="report[r_customthree]" placeholder="Custom Field Three">{{report.r_customthree}}</textarea>
    <br>

    <p class="field-description">Recommendation</p>
    <textarea class="field-input comments-input" name="report[r_recommendation]" placeholder="Recommendation">{{report.r_recommendation}}</textarea>
    <br>

    <p class="field-description">Observer's Name (i.e. Your Name)</p>
    <textarea class="field-input comments-input" name="report[counselor_name]" placeholder="Observer's Name">{{report.counselor_name}}</textarea>
    <br>

    <input class="submit-btn current" name="submit" id="edit-report-writeup-btn" type="submit" value="Save Edit" data-current-student-id="" data-current-session-id="">
  </fieldset>
</form>
<br><br>

<div class="dashboard-container">
  <input id="dashboard-home-btn" type="button" value="View Records">
  <input id="dashboard-new-session-btn" type="button" value="Create Record">
</div>
