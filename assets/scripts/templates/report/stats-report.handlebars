<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h1>Session Report</h1>
    </div>
  </div>

  <div class="row">
    <div class="col-xs-12 display-student-summary-container"></div>
  </div>

  <div class="row">
    <div class="col-xs-12 display-stats-table-container"></div>
  </div>

  <div class="row">
    <div class="col-xs-12 display-observation-container"></div>
  </div>

  <div class="row">
    <div class="col-md-6 col-xs-12 et-chart-width">
      <div class="et-container">
        <canvas class="results-chart" id="et-chart" width="100" height="100"></canvas>
      </div>
    </div>

    <div class="col-md-6 oft-chart-width">
      <div class="oft-container">
        <canvas class="results-chart" id="oft-chart" width="100" height="100"></canvas>
      </div>
    </div>
  </div>

  <div class="clearfix"></div>

  <div class="row">
    <div class="col-xs-12">
      <div class="dashboard-container">
        <button id="dashboard-home-btn" type="button" class="btn btn-primary">View Dashboard</button>
        <button id="dashboard-new-session-btn" type="button" class="btn btn-success">Create Session</button>
      </div>
    </div>
  </div>
</div>
