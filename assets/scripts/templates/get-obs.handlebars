<table class="table obs-summary-table table-hover">
  <thead>
    <tr>
      <th>Interval</th>
      <th>AET</th>
      <th>PET</th>
      <th>OFT-M</th>
      <th>OFT-V</th>
      <th>OFT-P</th>
      <th>Comment</th>
      <th>Update</th>
    </tr>
  </thead>
  {{#each observations as |observation|}}
    <tr>
      <tbody>
        <td class="display-obs-num">{{observation.obs_num}}</td>
        <td class="display-obs-aet">
          {{#if observation.aet}}
            &#10004;
          {{/if}}
        </td>
        <td class="display-obs-pet">
          {{#if observation.pet}}
            &#10004;
          {{/if}}
        </td>
        <td class="display-obs-oft-m">
          {{#if observation.oft_m}}
            &#10004;
          {{/if}}
        </td>
        <td class="display-obs-oft-v">
          {{#if observation.oft_v}}
            &#10004;
          {{/if}}
        </td>
        <td class="display-obs-oft-p">
          {{#if observation.oft_p}}
            &#10004;
          {{/if}}
        </td>
        <td class="display-obs-obs-comment">
          {{observation.obs_comment}}
        </td>
        <td><button data-id="{{observation.id}}" data-obs-num="{{observation.obs_num}}" data-aet="{{observation.aet}}" data-pet="{{observation.pet}}" data-oft-m="{{observation.oft_m}}" data-oft-v="{{observation.oft_v}}" data-oft-p="{{observation.oft_p}}" data-obs-comment="{{observation.obs_comment}}" data-current-session-id="{{observation.session_id}}" data-current-student-id="{{observation.student_id}}">Edit</button></td>
    </tr>
    </tbody>
  {{/each}}
</table>
